{
    "compilerOptions": {
      "target": "ES2015",
      "lib": [
        "es2019",
        "DOM"
      ],
      "module": "commonjs",
      "declaration": true,
      "outDir": "./dist",
      "strict": true,
      "strictNullChecks": false,
      "paths": {
        "vs/*": [
          "./src/vs/*"
        ],
      }
    },
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "node_modules"
    ]
}